:- pragma termination2_info(inf_const_bug.bar((builtin.in)) = (builtin.out), constraints([le([term(0, r(-1, 1))], r(-1, 1)), le([term(1, r(-1, 1))], r(-1, 1))]), not_set, can_loop).
