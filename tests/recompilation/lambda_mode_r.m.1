:- module lambda_mode_r.

:- interface.

:- import_module io.

:- pred main(io::di, io::uo) is det.

:- implementation.

:- import_module lambda_mode_r_2.

main(!IO) :-
    P =
        ( pred(X :: foo_in, di, uo) is det -->
            io.write_line(X)
        ),
    init_foo(F),
    P(F, !IO).

:- pred output_foo(foo::in, io::di, io::uo) is det.

output_foo(X, !IO) :-
    io.write_line(X, !IO).
