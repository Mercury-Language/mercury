INSTALL_PREFIX := $(INSTALL_PREFIX)/extras

-include ../Mmake.params

LIBRARY = libmercury_www

MAIN_TARGET = $(LIBRARY)

MCFLAGS-mercury_www = --no-warn-nothing-exported

depend: mercury_www.depend

distclean: realclean

install: libmercury_www.install

.PHONY: check
check:
	true
